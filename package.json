    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "sass": "^1.71.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",

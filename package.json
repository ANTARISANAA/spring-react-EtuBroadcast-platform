    "i18next": "^24.0.1",
    "i18next-browser-languagedetector": "^8.0.2",
    "qs": "^6.14.0",
    "react": "^19.1.0",
    "react-async-states": "^2.0.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.0.1",
    "react-router-dom": "^6.28.0"
